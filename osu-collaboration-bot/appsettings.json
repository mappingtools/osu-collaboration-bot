{
  "App": {
    "ConnectionString": "",
    "Path": "/etc/osu_collab_bot/",
    "Token": "",
    "Prefix": "mt!"
  },
  "NLog": {
    "throwConfigExceptions": true,
    "targets": {
      "async": true,
      "logfile": {
        "type": "File",
        "fileName": "/etc/logs/nlog-${shortdate}.log",
        "maxArchiveFiles": 4,
        "archiveAboveSize": 10240
      },
      "logconsole": {
        "type": "Console"
      }
    },
    "rules": [
      {
        "logger": "*",
        "minLevel": "Debug",
        "writeTo": "logconsole"
      },
      {
        "logger": "*",
        "minLevel": "Debug",
        "writeTo": "logfile"
      }
    ]
  }
}
